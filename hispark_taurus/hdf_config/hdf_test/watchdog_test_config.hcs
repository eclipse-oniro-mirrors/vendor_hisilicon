root {
    module = "watchdog";
    watchdog_config {
        watchdog_device_tester {
            match_attr = "WATCHDOG_TEST";
            id = 7;
            timeoutSet = 2;
            statusStop = 0;
            statusStart = 0;
            feedTime = 6;
        }
        watchdog_device_virtual {
            match_attr = "virtual_watchdog";
            id = 7;
            timeoutSet = 2;
            statusStop = 0;
            statusStart = 0;
            feedTime = 6;
        }
    }
}
