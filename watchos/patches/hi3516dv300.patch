+    dev->mode_config.min_width = 0;
+    dev->mode_config.min_height = 0;
+    dev->mode_config.max_width = HI_DRM_MAX_WIDTH;
+    dev->mode_config.max_height = HI_DRM_MAX_HEIGHT;
