+static inline void check_i2c_send_complete(struct hibvt_i2c_dev *i2c)
+{
+	unsigned int val;
+	val = readl(i2c->base + HIBVT_I2C_GLB);
+	if(val & GLB_EN_MASK){
+		hibvt_i2c_wait_idle(i2c);
+		hibvt_i2c_disable(i2c);
+	}
+}
