+
+static struct platform_driver hisi_nand_driver = {
+	.driver = {
+		.name   = "hisi_nand",
+		.of_match_table = hisi_nand_dt_ids,
+	},
+	.probe  = hisi_nand_os_probe,
+	.remove = hisi_nand_os_remove,
+#ifdef CONFIG_PM
+	.suspend    = hifmc100_nand_os_suspend,
+	.resume     = hifmc100_nand_os_resume,
+#endif
+};
