+    struct hi_drm_crtc crtcs[HI_DRM_MAX_CRTC_NUM];
+    struct drm_hal_disp_dev *disp_dev; /* the pointer to drm hal display api */
+    struct drm_hal_gfx_dev *gfx_dev; /* the pointer to drm hal graphics api */
