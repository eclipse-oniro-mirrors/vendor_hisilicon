diff --git a/drivers/i2c/busses/i2c-hibvt.c b/drivers/i2c/busses/i2c-hibvt.c
new file mode 100644
index 000000000..4c7cd494a
--- /dev/null
+++ b/drivers/i2c/busses/i2c-hibvt.c
@@ -0,0 +1,1451 @@
+/*
+ * Hisilicon BVT I2C Controller Driver
+ *
